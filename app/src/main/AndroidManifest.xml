<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.webabcd.androiddemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.BasicDemo1" />
        <activity android:name=".ui.BasicDemo1" />
        <activity android:name=".service.BasicDemo1" />

        <service android:name=".service.DownloadService" />

        <activity android:name=".view.text.TextViewDemo1" />
        <activity android:name=".view.text.TextViewDemo2" />
        <activity android:name=".view.text.TextViewDemo3" />
        <activity android:name=".view.text.TextViewDemo4" />
        <activity android:name=".view.text.TextViewDemo5"></activity>
    </application>

</manifest>